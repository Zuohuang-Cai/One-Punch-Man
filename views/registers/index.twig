{% extends "layouts/app.twig" %}

{% block header %}
    <head>
        <meta charset="UTF-8">
        <meta name="viewport"
              content="width=device-width, user-scalable=no, initial-scale=1.0, maximum-scale=1.0, minimum-scale=1.0">
        <meta http-equiv="X-UA-Compatible" content="ie=edge">
        <title>Document</title>
        <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/css/bootstrap.min.css" rel="stylesheet"
              integrity="sha384-T3c6CoIi6uLrA9TneNEoa7RxnatzjcDSCmG1MXxSR1GAsXEV/Dwwykc2MPK8M2HN"
              crossorigin="anonymous">
        <link rel="stylesheet" href="/assets/css/register.css">
    </head>

{% endblock %}
{% block body %}
    <div class="container">
        <form action="/register/createadmin" method="post">
            <input type="text" name="username" required placeholder="userame"><br><br>
            <input type="text" name="password" required placeholder="password"><br><br>
            <input type="text" name="confirm" required placeholder="confirm password"><br><br>
            <input type="submit" name="update" required placeholder="Register"><br><br>
        </form>
    </div>
{% endblock %}

